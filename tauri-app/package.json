{
  "name": "tauri-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "php-server": "php -S localhost:8088",
    "dev-with-php": "concurrently \"npm run dev\" \"npm run php-server\" --names \"VITE,PHP\" --prefix-colors \"cyan,yellow\"",
    "tauri-with-php": "concurrently \"npm run tauri dev\" \"npm run php-server\" --names \"TAURI,PHP\" --prefix-colors \"green,yellow\""
  },
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "concurrently": "^9.2.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.3"
  }
}
